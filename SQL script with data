/****** Object:  Table [dbo].[Alarms]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[Alarms](
	[Timestamp] [datetime] NOT NULL,
	[AlarmId] [int] NOT NULL,
	[Value] [real] NOT NULL,
	[Acked] [bit] NOT NULL,
 CONSTRAINT [PK_Alarms] PRIMARY KEY CLUSTERED 
(
	[Timestamp] ASC,
	[AlarmId] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  Table [dbo].[Alarmtypes]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[Alarmtypes](
	[AlarmId] [int] IDENTITY(1,1) NOT NULL,
	[Alarm Type] [varchar](50) NOT NULL,
	[Alarm Description] [varchar](max) NULL,
	[Alarm Limit] [real] NULL,
PRIMARY KEY CLUSTERED 
(
	[AlarmId] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY] TEXTIMAGE_ON [PRIMARY]
GO
/****** Object:  View [dbo].[AlarmHistory]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[AlarmHistory]
AS
SELECT        TOP (100) PERCENT dbo.Alarms.Timestamp, dbo.Alarmtypes.[Alarm Type], dbo.Alarmtypes.[Alarm Description], dbo.Alarms.Value, dbo.Alarmtypes.[Alarm Limit], dbo.Alarms.Acked
FROM            dbo.Alarms INNER JOIN
                         dbo.Alarmtypes ON dbo.Alarms.AlarmId = dbo.Alarmtypes.AlarmId
WHERE        (NOT (dbo.Alarms.Acked = 0))
ORDER BY dbo.Alarms.Timestamp DESC
GO
/****** Object:  Table [dbo].[Timestamp]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[Timestamp](
	[Timestamp] [datetime] NOT NULL,
 CONSTRAINT [PK_Timestamp] PRIMARY KEY CLUSTERED 
(
	[Timestamp] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[ActiveAlarms]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[ActiveAlarms]
AS
SELECT        TOP (100) PERCENT dbo.Alarms.Timestamp, dbo.Alarmtypes.[Alarm Type], dbo.Alarmtypes.[Alarm Description], dbo.Alarms.Value, dbo.Alarmtypes.[Alarm Limit], dbo.Alarms.Acked
FROM            dbo.Alarms INNER JOIN
                         dbo.Alarmtypes ON dbo.Alarms.AlarmId = dbo.Alarmtypes.AlarmId INNER JOIN
                         dbo.Timestamp ON dbo.Alarms.Timestamp = dbo.Timestamp.Timestamp
WHERE        (NOT (dbo.Alarms.Acked = 1))
ORDER BY dbo.Alarms.Timestamp DESC
GO

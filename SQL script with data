ALTER TABLE [dbo].[Alarms]  WITH NOCHECK ADD  CONSTRAINT [FK_Alarms_Alarmtypes] FOREIGN KEY([AlarmId])
REFERENCES [dbo].[Alarmtypes] ([AlarmId])
GO
ALTER TABLE [dbo].[Alarms] NOCHECK CONSTRAINT [FK_Alarms_Alarmtypes]
GO
ALTER TABLE [dbo].[Alarms]  WITH NOCHECK ADD  CONSTRAINT [FK_Alarms_Timestamp] FOREIGN KEY([Timestamp])
REFERENCES [dbo].[Timestamp] ([Timestamp])
GO
ALTER TABLE [dbo].[Alarms] NOCHECK CONSTRAINT [FK_Alarms_Timestamp]
GO
ALTER TABLE [dbo].[Data]  WITH NOCHECK ADD  CONSTRAINT [FK_Data_DataType] FOREIGN KEY([DataId])
REFERENCES [dbo].[DataType] ([DataId])
GO
ALTER TABLE [dbo].[Data] CHECK CONSTRAINT [FK_Data_DataType]
GO
ALTER TABLE [dbo].[Data]  WITH NOCHECK ADD  CONSTRAINT [FK_Data_Timestamp] FOREIGN KEY([Timestamp])
REFERENCES [dbo].[Timestamp] ([Timestamp])
GO
ALTER TABLE [dbo].[Data] CHECK CONSTRAINT [FK_Data_Timestamp]
GO
/****** Object:  StoredProcedure [dbo].[AckAllAlarms]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[AckAllAlarms]
AS
Begin
update Alarms
set 
Acked = 1
where Acked=0
end
GO
/****** Object:  StoredProcedure [dbo].[AddAlarm]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[AddAlarm]
@Time DATETIME,
@Value real,
@ID	int

as
Insert into Timestamp
Values(@Time,'Alarm')

insert into Alarms 
values(@Time,@ID,@Value)

GO
/****** Object:  StoredProcedure [dbo].[ViewActiveAlarms]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[ViewActiveAlarms]
	
AS
BEGIN
	SELECT * from ActiveAlarms
END
GO

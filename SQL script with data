CREATE TABLE [dbo].[DataType](
	[DataId] [int] NOT NULL,
	[Data Text] [varchar](50) NULL,
	[Data Info] [varchar](max) NULL,
 CONSTRAINT [PK_DataType] PRIMARY KEY CLUSTERED 
(
	[DataId] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY] TEXTIMAGE_ON [PRIMARY]
GO
/****** Object:  View [dbo].[DataHistory]    Script Date: 28/09/2020 20:20:38 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[DataHistory]
AS
SELECT        dbo.Timestamp.Timestamp, dbo.DataType.[Data Text], dbo.DataType.[Data Info], dbo.Data.Value
FROM            dbo.Data INNER JOIN
                         dbo.DataType ON dbo.Data.DataId = dbo.DataType.DataId INNER JOIN
                         dbo.Timestamp ON dbo.Data.Timestamp = dbo.Timestamp.Timestamp
GO
